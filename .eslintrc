{
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "rules": {
    "comma-dangle": "off",
    "semi": "off",
    "@typescript-eslint/semi": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "@typescript-eslint/space-before-function-paren": "off",
    "no-console": "error"
  }
}
